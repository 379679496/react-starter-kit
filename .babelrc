{
  "presets": [
    "react",
    [
      "env",
      {
        "modules": false,
        "useBuiltIns": false,
        "targets": {
          "browsers": ["last 2 versions", "ie >= 7"]
        }
      }
    ]
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-runtime",
    "transform-object-rest-spread",
    "babel-plugin-syntax-dynamic-import",
    ["import", { "libraryName": "antd", "style": "css" }]
  ]
}